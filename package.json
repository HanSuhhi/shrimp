{
  "name": "nuxt-app",
  "type": "module",
  "version": "0.0.1",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "private": true,
  "scripts": {
    "build": "NODE_ENV=production nuxt build",
    "dev": "nuxt dev",
    "cz": "git-cz",
